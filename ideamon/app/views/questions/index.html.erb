<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">

      <div class="page-header">          
        <h1>Questionaire Results</h1>
      </div>        


        <div class="well">
          <%= link_to 'Export XLS', url_for(:format => 'xls'), class: "btn btn-primary btn-sm" %> 
          <%= link_to 'Export CSV', url_for(:format => 'csv'), class: "btn btn-primary btn-sm" %>
          </br>
          </br>         
          <div class="panel panel-info">
            <h3></h3>
            <div class="panel-body">       
              <div class="table-responsive table-scrollable">
                <table class="table table-striped table-bordered table-hover ">
                  <thead>
                    <tr>
                      <th>Q-ID</th>
                      <th>F1</th>
                      <th>F2</th>
                      <th>F3</th>
                      <th>F4</th>
                      <th>F5</th>
                      <th>F6</th>
                      <th>F7</th>
                      <th>F8</th>
                      <th>F9</th>
                      <th>F10</th>
                      <th>F11</th>
                      <th>F12</th>
                      <th>F13</th>
                      <th>F14</th>
                      <th>F15</th>
                      <th>F16</th>
                      <th>F17</th>
                      <th>F18</th>
                      <th>UserID</th>
                      <th>Username</th>
                      <th>User-Email</th>
                      <th>Age</th>
                      <th>Sex</th>
                      <th>Nationalit√§t</th>
                      <th>Abschluss</th>
                      <th>Beruf</th>
                      <th>Created_at</th>
                      <th>Updated_at</th>
                      <th colspan="3"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @questions.each do |question| %>
                      <tr>
                        <td><%= question.id %></td>
                        <td><%= question.f1 %></td>
                        <td><%= question.f2 %></td>
                        <td><%= question.f3 %></td>
                        <td><%= question.f4 %></td>
                        <td><%= question.f5 %></td>
                        <td><%= question.f6 %></td>
                        <td><%= question.f7 %></td>
                        <td><%= question.f8 %></td>
                        <td><%= question.f9 %></td>
                        <td><%= question.f10 %></td>
                        <td><%= question.f11 %></td>
                        <td><%= question.f12 %></td>
                        <td><%= question.f13 %></td>
                        <td><%= question.f14 %></td>
                        <td><%= question.f15 %></td>
                        <td><%= question.f16 %></td>
                        <td><%= question.f17 %></td>
                        <td><%= question.f18 %></td>
                        <td><%= question.user_id %></td>
                        <td><%= question.user.username %></td>
                        <td><%= question.user.email %></td>
                        <td><%= question.user.age %></td>
                        <td><%= question.user.sex %></td>
                        <td><%= question.user.nationality %></td>
                        <td><%= question.user.qualification %></td>
                        <td><%= question.user.profession %></td>
                        <td><%= question.created_at %></td>
                        <td><%= question.updated_at %></td> 
                        <td><%= link_to 'Show', question %></td>
                        <td><%= link_to 'Edit', edit_question_path(question) %></td>
                        <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                      </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
              <br>     
            </div>
          </div>
          <%= link_to 'New Questionaire', new_question_path %>
        </div>


    </div>
  </div>
</div>



